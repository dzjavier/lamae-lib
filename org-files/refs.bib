@ARTICLE{Adur2004,
  author = {Javier Adur and Tsuyoshi Uchide and Satoshi Takizawa and Jiexia Quan
	and Kaname Saida},
  title = {Real-time Polymerase Chain Reaction Quantification of Gene Expression
